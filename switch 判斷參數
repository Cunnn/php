    <?php
		$number = 6;
		switch($number)
		{
			case 1:
				echo'ONe';
				break;
			case 2:
				echo'TWO';
				break;
			case 3:
				echo'three';
				break;
			case 4:
				echo'four';
				break;			
			default:
				echo'數值超過範圍';				
	}
    ?>
  </body>
